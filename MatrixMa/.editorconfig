[*.cs]

# CS8618: Un campo que no acepta valores NULL debe contener un valor distinto de NULL al salir del constructor. Considere la posibilidad de declararlo como que admite un valor NULL.
dotnet_diagnostic.CS8618.severity = silent

# CS8602: Desreferencia de una referencia posiblemente NULL.
dotnet_diagnostic.CS8602.severity = none

# CS0661: El tipo define operator == or operator !=, pero no reemplaza a Object.GetHashCode()
dotnet_diagnostic.CS0661.severity = none

# CS0660: El tipo define operator == or operator !=, pero no reemplaza a override Object.Equals(object o)
dotnet_diagnostic.CS0660.severity = none
